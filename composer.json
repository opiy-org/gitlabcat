{
    "name": "opiy-org/gitlabcat",
    "description": "GitLab events, issues, statistics, etc notifications via Telegram bot.",
    "keywords": ["gitlab", "telegram", "bot"],
    "license": "GPL3",
    "authors": [
      {
        "name": "opiy",
        "email": "opiy@opiy.org"
      }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "botman/botman": ">2.1",
        "botman/driver-telegram": "1.3",
        "doctrine/dbal": ">=2.7",
        "predis/predis": "^1.1",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "nunomaduro/collision": "~1.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "barryvdh/laravel-ide-helper": "^2.4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
